{
  "name": "pocket2kindle",
  "version": "1.0.0",
  "description": "send unread pocket articles to kindle",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "dev": "webpack --config webpack.dev.js",
    "watch": "webpack --watch --progress --config webpack.dev.js",
    "dev-server": "nodemon ./server.js",
    "dev-client": "webpack-dev-server --open --config webpack.dev.js",
    "start": "concurrently -p \"[{name}]\" -n \"dev-server,dev-client,webpack\" -c \"yellow.bold,blue.bold,magenta.bold,\" \"npm run dev-server\" \"npm run dev-client\" \"npm run watch\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Paul Thomas",
  "license": "MIT",
  "browserslist": [
    "last 3 versions",
    "ie 11"
  ],
  "dependencies": {
    "express": "^4.16.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "clean-webpack-plugin": "^0.1.17",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.7.2",
    "nodemon": "^1.14.2",
    "postcss-loader": "^2.0.10",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7",
    "webpack-merge": "^4.1.1"
  }
}
