{
  "name": "pocket2kindle",
  "version": "1.0.0",
  "description": "send unread pocket articles to kindle",
  "repository": {
    "type": "git",
    "url": "https://github.com/planetflash/pocket2kindle"
  },
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "dev": "webpack --config webpack.dev.js",
    "watch": "webpack --watch --progress --config webpack.dev.js",
    "dev-server": "nodemon ./server.js",
    "dev-client": "webpack-dev-server --open --config webpack.dev.js",
    "start": "concurrently -p '[{name}]' -n 'dev-server,dev-client,webpack' -c 'yellow.bold,blue.bold,magenta.bold,' 'npm run dev-server' 'npm run dev-client' 'npm run watch'",
    "test": "jest"
  },
  "author": "Paul Thomas",
  "license": "MIT",
  "dependencies": {
    "express": "^4.16.2",
    "node-getpocket": "^1.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.4",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "classnames": "^2.2.5",
    "clean-webpack-plugin": "^0.1.17",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^22.0.4",
    "lint-staged": "^6.1.1",
    "lodash": "^4.17.5",
    "node-sass": "^4.7.2",
    "nodemon": "^1.14.2",
    "postcss-loader": "^2.0.10",
    "prettier": "^1.10.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^3.1.3",
    "react-redux": "^5.0.6",
    "react-router": "^3.2.0",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "redux-saga": "^0.16.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.1",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7",
    "webpack-merge": "^4.1.1"
  }
}
